RewriteEngine On
RewriteRule ^/?([a-zA-Z_]+)/([a-zA-Z_]+)/([0-9]+)$ Projeto/web.php?metodo=$1&arg1=$2&arg2=$3 [L]
RewriteRule ^/?([a-zA-Z_]+)/([0-9]+)$ Projeto/web.php?metodo=$1&arg1=$2 [L]
RewriteRule ^/?([a-zA-Z_]+)$ Projeto/web.php?metodo=$1 [L]
RewriteRule ^/?([a-zA-Z_]+)/([a-zA-Z_]+)$ digiudo/app.php?classe=$1&metodo=$2 [L,QSA]


